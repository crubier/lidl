interaction
  (empty UI with cursor):{
        dimension: {
          width: Number in,
          height: Number in
        },
        time: Number in,
        mouse: {
          buttons: Number in,
          position: {
            x: Number in,
            y: Number in
          },
          wheel: {
            x: Number in,
            y: Number in,
            z: Number in
          }
        },
        keyboard: {
          enter: Boolean in,
        },
        graphics: Graphics out
      }

with

  interaction
    (cursor of (mouse:Mouse in)):Graphics out
  is
    ((#cursor of (mouse))
    with behaviour
    ((function cursor)(mouse)=(#cursor of (mouse))))


is
  ({
        dimension: (#dimension)
        time:(#time)
        mouse: (#mouse)
        keyboard: (#keyboard)
        graphics: (cursor of (#mouse))
  })
