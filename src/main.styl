 @import "reset"



 html,body {
    font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
    font-weight: 300;
    height: 100%;
    width:100%;
    overflow:hidden;
 }

 div {
   vertical-align: middle;
   height: 30%;
 }

 textarea {
   width:100%;
   height:100%;
   border:none;
   margin:0;
   resize: both;
   box-sizing: border-box;
   font-family:"Courier New", Courier, monospace;
   background-color:rgb(200, 255, 252);
   font-size: 13pt;
 }

 textarea.error {
   background-color:rgb(255, 191, 191);
 }

 textarea:focus, input:focus{
    outline: 0;
}

span {
  display: inline-block;
  vertical-align: middle;
  text-align:center;
  line-height:normal;
}

.CodeEditor {
  display:inline-block;
float:left;
width:50%;
height:100%;
background-color:rgb(116, 244, 255);

}

.TraceViewer {
  display:inline-block;
float:left;
width:50%;
height:100%;
background-color:rgb(255, 250, 178);
vertical-align: middle;
text-align:center;
display:table-cell;
overflow:auto;
}

.Main {
  position:absolute;
  left:0px;
  right:0px;
width:100%;
height:100%;
background-color:rgb(255, 178, 254);
}

.Tabs {
width:100%;
height:60px;
float: left;
}

.Tab {
  display:inline-block;
width:33.33%;
height:100%;
margin:0;
vertical-align: middle;
text-align:center;
line-height:60px;
}

.Tab.active {
  background-color:rgb(200, 255, 252);
}

.Tab:hover {
  background-color:rgb(135, 171, 255);
}

.TabContent {
  float:left;
width:100%;
height:calc(100% - 60px);
}

.button{
  position: relative;
  top: -43px;
  left: 410px;
}

button {
  background: #fcfcfc;
  background-image: -webkit-linear-gradient(top, #fcfcfc, #e3e5e6);
  background-image: -moz-linear-gradient(top, #fcfcfc, #e3e5e6);
  background-image: -ms-linear-gradient(top, #fcfcfc, #e3e5e6);
  background-image: -o-linear-gradient(top, #fcfcfc, #e3e5e6);
  background-image: linear-gradient(to bottom, #fcfcfc, #e3e5e6);
  -webkit-border-radius: 28;
  -moz-border-radius: 28;
  border-radius: 28px;
  font-family: Arial;
  color: #10e6bb;
  font-size: 15px;
  padding: 10px 20px 10px 20px;
  text-decoration: none;
}

button:hover {
  background: #f5f5f5;
  background-image: -webkit-linear-gradient(top, #f5f5f5, #b6b7b8);
  background-image: -moz-linear-gradient(top, #f5f5f5, #b6b7b8);
  background-image: -ms-linear-gradient(top, #f5f5f5, #b6b7b8);
  background-image: -o-linear-gradient(top, #f5f5f5, #b6b7b8);
  background-image: linear-gradient(to bottom, #f5f5f5, #b6b7b8);
  text-decoration: none;
}




