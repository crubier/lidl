interaction
  (bob):{theNumber:Number in,theResult:Number out}
with
  interaction
    (bob(x:Number in)):Number out
  with
    interaction
      (addOne):{Number->Number}out
    is
      (function addOne)
  is
    ((result of bob(x)) with behaviour ((addOne)(x)=(result of bob(x))))
is
  ({theNumber:(a),theResult:(bob(a))})
